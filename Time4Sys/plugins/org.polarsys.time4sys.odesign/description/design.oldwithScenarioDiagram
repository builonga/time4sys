                        </subModelOperations>
                      </subModelOperations>
                    </subModelOperations>
                  </subModelOperations>
                  <subModelOperations xsi:type="tool:For" expression="aql:element.inputRel.predec" iteratorName="step">
                    <subModelOperations xsi:type="tool:If" conditionExpression="aql:step.oclIsKindOf(gqam::Step)">
