                        <subModelOperations xsi:type="tool:ChangeContext" browseExpression="aql:element.outputRel.succes->first()">
                          <subModelOperations xsi:type="tool:SetValue" featureName="inputRel" valueExpression="aql:step.inputRel"/>
                        </subModelOperations>
                        <subModelOperations xsi:type="tool:ChangeContext" browseExpression="var:step">
                          <subModelOperations xsi:type="tool:ChangeContext" browseExpression="&lt;%outputRel%>">
                            <subModelOperations xsi:type="tool:RemoveElement"/>
                          </subModelOperations>
                          <subModelOperations xsi:type="tool:RemoveElement"/>
                        </subModelOperations>
                      </subModelOperations>
                    </subModelOperations>
                  </subModelOperations>
                  <subModelOperations xsi:type="tool:For" expression="aql:element.outputRel.succes" iteratorName="step">
                    <subModelOperations xsi:type="tool:If" conditionExpression="aql:step.oclIsKindOf(gqam::Step) and (step.repetitions=-1)">
                      <subModelOperations xsi:type="tool:ChangeContext" browseExpression="var:step">
                        <subModelOperations xsi:type="tool:ChangeContext" browseExpression="feature:inputRel"/>
                        <subModelOperations xsi:type="tool:RemoveElement"/>
                      </subModelOperations>
                    </subModelOperations>
                  </subModelOperations>
                </subModelOperations>
                <subModelOperations xsi:type="tool:If" conditionExpression="aql:element.inputRel.predec->size()=0">
                  <subModelOperations xsi:type="tool:If" conditionExpression="aql:element.inputRel!=null">
                    <subModelOperations xsi:type="tool:ChangeContext" browseExpression="aql:element.inputRel">
                      <subModelOperations xsi:type="tool:RemoveElement"/>
                    </subModelOperations>
                  </subModelOperations>
                </subModelOperations>
                <subModelOperations xsi:type="tool:If" conditionExpression="aql:element.outputRel.succes->size()=0">
                  <subModelOperations xsi:type="tool:If" conditionExpression="aql:element.outputRel!=null">
                    <subModelOperations xsi:type="tool:ChangeContext" browseExpression="aql:element.outputRel">
