                          <subModelOperations xsi:type="tool:RemoveElement"/>
                        </subModelOperations>
                        <subModelOperations xsi:type="tool:RemoveElement"/>
                      </subModelOperations>
                    </subModelOperations>
                  </subModelOperations>
                </subModelOperations>
                <subModelOperations xsi:type="tool:If" conditionExpression="aql:element.outputRel->size()!=0">
                  <subModelOperations xsi:type="tool:If" conditionExpression="aql:element.outputRel.predec->size()>1">
                    <subModelOperations xsi:type="tool:For" expression="aql:element.outputRel.predec" iteratorName="step">
