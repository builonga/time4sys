<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated constraints -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="org.polarsys.time4sys.constraints.model.constraints.presentation.ConstraintsModelWizardID"
            name="%_UI_ConstraintsModelWizard_label"
            class="org.polarsys.time4sys.constraints.model.editor.presentation.ConstraintsModelWizard"
