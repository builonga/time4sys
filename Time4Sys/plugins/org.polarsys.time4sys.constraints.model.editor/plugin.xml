         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated constraints -->
      <editor
            id="org.polarsys.time4sys.constraints.model.constraints.presentation.ConstraintsEditorID"
            name="%_UI_ConstraintsEditor_label"
            icon="icons/full/obj16/ConstraintsModelFile.gif"
            extensions="constraints"
            class="org.polarsys.time4sys.constraints.model.editor.presentation.ConstraintsEditor"
            contributorClass="org.polarsys.time4sys.constraints.model.editor.presentation.ConstraintsActionBarContributor">
      </editor>
   </extension>

</plugin>
